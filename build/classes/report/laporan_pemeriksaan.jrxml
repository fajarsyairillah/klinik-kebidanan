	<queryString>
		<![CDATA[SELECT
     periksa.`no_rm` AS periksa_no_rm,
     periksa.`nama` AS periksa_nama,
     periksa.`nama_tindakan` AS periksa_nama_tindakan,
     periksa.`ruang_bersalin` AS periksa_ruang_bersalin,
     periksa.`harga_ruang` AS periksa_harga_ruang,
     periksa.`keadaan_bayi` AS periksa_keadaan_bayi,
     periksa.`jenkel_bayi` AS periksa_jenkel_bayi,
     periksa.`nama_periksa` AS periksa_nama_periksa,
     periksa.`diagnosa` AS periksa_diagnosa,
     periksa.`daftar_obat` AS periksa_daftar_obat,
     periksa.`tanggal` AS periksa_tanggal,
     periksa.`jam` AS periksa_jam,
     periksa.`no_periksa` AS periksa_no_periksa
FROM
     `periksa` periksa
WHERE
periksa.tanggal BETWEEN $P{tgl1} AND $P{tgl2}]]>
	</queryString>
